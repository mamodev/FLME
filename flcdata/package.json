{
    "name": "ui",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "start": "concurrently \"vite --config .uiconfig/vite.config.ts\" \"python src/server.py\"",
        "runpreview": "concurrently \"vite preview --config .uiconfig/vite.config.ts\" \"python src/server.py\"",
        "dev": "vite --config .uiconfig/vite.config.ts",
        "build": "NODE_OPTIONS=\"--max-old-space-size=28672 --max-semi-space-size=4096 --optimize_for_size --stack-size=4096\" tsc --noEmitOnError false  && vite build --config .uiconfig/vite.config.ts",
        "preview": "vite preview --host --config .uiconfig/vite.config.ts",
        "lint": "eslint ."
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/icons-material": "^7.0.2",
        "@mui/material": "^7.0.2",
        "@mui/x-data-grid": "^7.28.3",
        "@tanstack/react-query": "^5.72.2",
        "@types/react-plotly.js": "^2.6.3",
        "@xyflow/react": "^12.5.6",
        "mathjs": "^14.4.0",
        "plotly.js": "^3.0.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-markdown": "^10.1.0",
        "react-mosaic-component": "^6.1.1",
        "react-plotly.js": "^2.6.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.21.0",
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "@vitejs/plugin-react": "^4.3.4",
        "concurrently": "^9.1.2",
        "eslint": "^9.21.0",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^15.15.0",
        "rollup-plugin-node-polyfills": "^0.2.1",
        "typescript": "~5.7.2",
        "typescript-eslint": "^8.24.1",
        "vite": "^6.2.0",
        "vite-plugin-node-polyfills": "^0.23.0"
    }
}